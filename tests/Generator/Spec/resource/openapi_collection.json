  "components": {
    "schemas": {
      "Entry": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "userId": {
            "type": "integer"
          },
          "title": {
            "type": "string",
            "pattern": "[A-z]+",
            "minLength": 3,
            "maxLength": 16
          },
          "date": {
            "format": "date-time",
            "type": "string"
          }
        }
      },
      "EntryCollection": {
        "type": "object",
        "properties": {
          "entry": {
            "type": "array",
            "items": {
