            }
        },
        "DepartureBoard": {
            "type": "object",
            "required": [
                "Departure"
            ],
            "properties": {
                "Departure": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/DepartureOrArrival"
                    }
                }
            }
        },
        "ArrivalBoardResponse": {
            "type": "object",
            "required": [
                "DepartureBoard"
            ],
            "properties": {
