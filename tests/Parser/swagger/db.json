        },
        "DepartureOrArrival": {
            "type": "object",
            "required": [
                "name",
                "type",
                "stopid",
                "stop",
                "time",
                "date",
                "direction",
                "track",
                "JourneyDetailRef"
            ],
            "properties": {
                "name": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "stopid": {
                    "type": "integer",
                    "format": "int32"
                },
                "stop": {
                    "type": "string"
                },
                "time": {
                    "$ref": "#/definitions/LocalTime"
                },
                "date": {
                    "$ref": "#/definitions/LocalDate"
