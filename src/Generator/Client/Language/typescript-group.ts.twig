import {ResourceAbstract} from "sdkgen-client"
{% for className, resource in resources %}
import {{ className }} from "./{{ className }}";
{% endfor %}

export default class {{ className }} extends ResourceAbstract {
{% for className, resource in resources %}
    /**
     * {{ resource.description|raw }}
     */
    public {{ resource.methodName }}({% for name, arg in resource.properties %}{{ name }}: {{ arg.type|raw }}{% if not loop.last %}, {% endif %}{% endfor %}): {{ className }}
    {
        return new {{ className }}(
            {% for name, arg in resource.properties %}{{ name }},
            {% endfor %}
this.baseUrl,
            this.httpClient
        );
    }

{% endfor %}
}
